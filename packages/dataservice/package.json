{
  "name": "@marmadilemanteater/dataservice",
  "version": "1.0.0",
  "description": "dataservice library for my static site",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist/ && rm *.d.ts",
    "build": " vite build -- blog-posts && vite build -- projects && vite build -- tags && vite build -- links && vite build -- social-posts && mv ./dist/*/*.* ./dist/ "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@marmadilemanteater/jsdocs2dts": "workspace:^",
    "highlight.js": "^11.9.0",
    "jsdom": "^22.1.0",
    "xml2js": "^0.6.2"
  },
  "exports": {
    "./blog-posts": {
      "import": "./dist/blog-posts.js",
      "require": "./dist/blog-posts.cjs",
      "types": "./blog-posts.d.ts",
      "default": "./dist/blog-posts.js"
    },
    "./projects": {
      "import": "./dist/projects.js",
      "require": "./dist/projects.cjs",
      "types": "./projects.d.ts",
      "default": "./dist/projects.js"
    },
    "./tags": {
      "import": "./dist/tags.js",
      "require": "./dist/tags.cjs",
      "types": "./tags.d.ts",
      "default": "./dist/tags.js"
    },
    "./links": {
      "import": "./dist/links.js",
      "require": "./dist/links.cjs",
      "types": "./links.d.ts",
      "default": "./dist/links.js"
    },
    "./social-posts": {
      "import": "./dist/social-posts.js",
      "require": "./dist/social-posts.cjs",
      "types": "./social-posts.d.ts",
      "default": "./dist/social-posts.js"
    }
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "vite": "^4.4.11"
  }
}
